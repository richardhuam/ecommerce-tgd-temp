{
  "name": "e-commerce-tgd-react-vite-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start:development": "env-cmd -f .env.development vite preview",
    "start:production": "env-cmd -f .env.production vite preview",
    "build:production": "env-cmd -f .env.production tsc && vite build",
    "lint:check": "eslint --ext .ts .",
    "lint:fix": "eslint --fix --ext .ts .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "serve": "serve -s dist -p 5173"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.2.0",
    "@nextui-org/react": "^2.0.22",
    "@reduxjs/toolkit": "^1.9.5",
    "apexcharts": "^3.42.0",
    "axios": "^1.4.0",
    "canvas-confetti": "^1.6.0",
    "dayjs": "^1.11.9",
    "env-cmd": "^10.1.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.268.0",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.4",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "swiper": "^10.2.0"
  },
  "devDependencies": {
    "@types/canvas-confetti": "^1.6.1",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.28",
    "prettier": "^3.0.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
